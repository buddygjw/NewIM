<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:util="http://www.springframework.org/schema/util" xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
    	http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.1.xsd
    	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">

    <context:annotation-config />
    <context:component-scan base-package="com.whosbean.newim.common"/>
    <context:component-scan base-package="com.whosbean.newim.zookeeper"/>
    <context:component-scan base-package="com.whosbean.newim.redis"/>
    <context:component-scan base-package="com.whosbean.newim.chatter"/>

</beans>
